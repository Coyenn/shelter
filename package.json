{
	"name": "shelter",
	"version": "0.0.1",
	"scripts": {
		"dev": "concurrently \"yarn svelte-kit-dev\" \"yarn storybook-start\"",
		"svelte-kit-dev": "svelte-kit dev -p 8000 --host 0.0.0.0",
		"svelte-kit-build": "svelte-kit build",
		"svelte-kit-package": "svelte-kit package",
		"svelte-kit-preview": "svelte-kit preview -p 8000 --host 0.0.0.0",
		"svelte-kit-prepare": "svelte-kit sync",
		"svelte-kit-check": "svelte-check --tsconfig ./tsconfig.json",
		"svelte-kit-check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"svelte-kit-lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"svelte-kit-format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"storybook-start": "start-storybook -p 8001",
		"storybook-build": "build-storybook"
	},
	"devDependencies": {
		"@babel/core": "7.17.8",
		"@faker-js/faker": "6.0.0",
		"@prisma/client": "3.11.0",
		"@storybook/addon-actions": "6.4.19",
		"@storybook/addon-essentials": "6.4.19",
		"@storybook/addon-links": "6.4.19",
		"@storybook/addon-svelte-csf": "1.1.0",
		"@storybook/svelte": "6.4.19",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-node": "1.0.0-next.73",
		"@sveltejs/kit": "next",
		"@types/cookie": "0.4.1",
		"@typescript-eslint/eslint-plugin": "5.16.0",
		"@typescript-eslint/parser": "5.16.0",
		"autoprefixer": "10.4.4",
		"babel-loader": "8.2.3",
		"concurrently": "7.0.0",
		"eslint": "8.11.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-storybook": "0.5.7",
		"eslint-plugin-svelte3": "3.4.1",
		"postcss": "8.4.12",
		"postcss-load-config": "3.1.3",
		"prettier": "2.6.1",
		"prettier-plugin-svelte": "2.6.0",
		"prisma": "3.11.0",
		"sass": "1.49.9",
		"storybook-builder-vite": "0.1.21",
		"svelte": "3.46.4",
		"svelte-check": "2.4.6",
		"svelte-hero-icons": "4.1.1",
		"svelte-loader": "3.1.2",
		"svelte-preprocess": "4.10.4",
		"tailwindcss": "3.0.23",
		"tslib": "2.3.1",
		"typescript": "4.6.2"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/fira-mono": "4.5.4",
		"@lukeed/uuid": "2.0.0",
		"cookie": "0.4.2"
	}
}
